<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mieandu.userservice.mapper.UserMapper">
    <select id="listUser" resultType="com.mieandu.userservice.model.User">
        select user_id userId,user_name userName
        from t_user
    </select>
    <select id="saveUser" parameterType="com.mieandu.userservice.model.User">
        insert into t_user (user_id, user_name, phone) values (#{userId}, #{userName}, #{phone})
    </select>
</mapper>